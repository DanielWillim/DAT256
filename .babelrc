{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["module-resolver", {
      "extensions": [".js", ".jsx"],
      "root": ["./src"],
      "alias": {
        "test": "./test"
      }
    }],
    "@babel/plugin-proposal-class-properties"
  ]
}
